<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/fragment_personal_iconround_width"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/iv_sign"
            android:layout_width="@dimen/deal_btn_high"
            android:layout_height="@dimen/deal_btn_high"
            android:layout_marginLeft="@dimen/iv_margin_left"
            android:src="@mipmap/jiechu"
            android:layout_gravity="center_vertical"/>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/carnum_icon_margin_left"
            android:layout_gravity="center_vertical">
            <ImageView
                android:id="@+id/iv_chepai"
                android:layout_width="@dimen/wuyi"
                android:layout_height="@dimen/yiba"
                android:src="@mipmap/che_pai_hao"
                android:layout_centerHorizontal="true"/>
            <TextView
                android:id="@+id/textView_carNum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/iv_chepai"
                android:layout_marginTop="@dimen/car_alarm_search_icon_margin"
                android:text=""
                android:textColor="@color/header_title_bg"
                android:layout_centerHorizontal="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/rl_time_margin_left"
            android:layout_gravity="center_vertical">
            <RelativeLayout
                android:id="@+id/rl_start"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/kaishi"
                    android:textSize="@dimen/time_text_size"/>
                <TextView
                    android:id="@+id/startTime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView1"
                    android:text=""
                    android:textSize="@dimen/time_text_size"
                    android:layout_marginLeft="@dimen/time_text_margin_left"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_end"
                android:layout_below="@+id/rl_start"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dialog_et_margin">
                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/jieshu"
                    android:textSize="@dimen/time_text_size"/>
                <TextView
                    android:id="@+id/endTime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView2"
                    android:text=""
                    android:textSize="@dimen/time_text_size"
                    android:layout_marginLeft="@dimen/time_text_margin_left"/>
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/half"
        android:background="@color/grey"/>

</LinearLayout>
