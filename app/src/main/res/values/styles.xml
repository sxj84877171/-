<resources>

    <!-- Base application theme. -->
    <!--<style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">-->
        <!--&lt;!&ndash; Customize your theme here. &ndash;&gt;-->
        <!--<item name="colorPrimary">@color/colorPrimary</item>-->
        <!--<item name="colorPrimaryDark">@color/colorPrimaryDark</item>-->
        <!--<item name="colorAccent">@color/colorAccent</item>-->
    <!--</style>-->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/colorAccent</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="MyTabLayoutTextAppearance" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">12sp</item>
        <item name="android:drawablePadding">0dp</item>
    </style>

    <style name="diolog_violation_address" parent="@android:style/Theme.Dialog">
        <!-- 设置未浮动窗口 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 设置无边框 -->

        <item name="android:windowFrame">@null</item>
        <!-- 设置无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 设置完全透明 -->
        <item name="android:windowBackground">@color/dialog_transparent_background</item>
        <!-- 设置屏幕变暗 -->
        <item name="android:backgroundDimEnabled">true</item>



    </style>
    <style name="Dialog" parent="android:style/Theme.Dialog">
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#888888</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <style name="text_first_word">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#888888</item>
    </style>

    <style name="text_content">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#3c3c3c</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="text_chejiahao">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#888888</item>
        <item name="android:layout_marginLeft">13.5dp</item>
    </style>

    <style name="ll">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="common_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">60dp</item>
        <item name="android:layout_marginRight">60dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize"> 16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/button_common_bg</item>
    </style>
    <style name="danger_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor"> #3c3c3c</item>
        <item name="android:textSize"> 13sp</item>
    </style>
    <style name="danger_haveobjection">
        <item name="android:layout_width">144dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:paddingLeft">40dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:paddingRight">40dp</item>
        <item name="android:textColor">#888888</item>
        <item name="android:textSize"> 16sp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@drawable/objection_have_bg</item>
    </style>

    <style name="danger_noobjection">
        <item name="android:layout_width">144dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:paddingLeft">40dp</item>
        <item name="android:paddingRight">40dp</item>
        <item name="android:textColor">@color/colorAccent_e</item>
        <item name="android:textSize"> 16sp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@drawable/objection_no_bg</item>
    </style>

    <style name="text_xianchang">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textColor">#3c3c3c</item>
    </style>

    <style name="tv_yangzhao">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/yangzhao_text_size</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">17dp</item>
        <item name="android:textColor">@color/yangzhao_color</item>
    </style>

    <style name="imageview">
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_width">67dp</item>
        <item name="android:layout_height">67dp</item>
    </style>

    <style name="drawer_left_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/border</item>
        <item name="android:layout_marginLeft">@dimen/img_margin</item>
    </style>

    <style name="drawer_left_next">
        <item name="android:layout_width">17dp</item>
        <item name="android:layout_height">17dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="text_content_use_record">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#3c3c3c</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">20dp</item>
    </style>

    <style name="use_car_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#3c3c3c</item>
    </style>

    <style name="user_car_text">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#3c3c3c</item>
    </style>

    <style name="ClusterIcon.TextAppearance" parent="android:TextAppearance">
        <item name="android:textColor">#ffeeeeee</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Bubble.TextAppearance.Dark" parent="android:TextAppearance.Medium">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#ff7f7f7f</item>
    </style>

    <style name="Bubble.TextAppearance.Light" parent="Bubble.TextAppearance.Dark">
        <item name="android:textColor">#ffeeeeee</item>
    </style>

    <style name="AnimationPreview">
        <item name="android:windowEnterAnimation">@anim/pop_push_up_in</item>
        <item name="android:windowExitAnimation">@anim/pop_push_down_out</item>
    </style>

    <style name="pop_text_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
    </style>

</resources>
